
// Definindo o modelo de dados para usuários
model User {
  id        Int      @id @default(autoincrement())
  username  String
  email     String   @unique
  password  String
  
}

// Definindo o modelo de dados para ferramentas de hacking ético
model EthicalHackingTool {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  downloadUrl String
}

